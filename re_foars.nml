//----------------------------------------------------------
//A switch; based on the value of 'view', 'switch_0_roadstop_view' gives different spritelayouts.
//----------------------------------------------------------
switch (FEAT_ROADSTOPS, SELF, switch_0_roadstop_view, view) {
 RST_VIEW_BAY_NE: spritelayout_0_roadstop_ne;
 RST_VIEW_BAY_SE: spritelayout_0_roadstop_se;
 RST_VIEW_BAY_SW: spritelayout_0_roadstop_sw;
 RST_VIEW_BAY_NW: spritelayout_0_roadstop_nw;
 RST_VIEW_DRIVE_THROUGH_X: spritelayout_0_roadstop_x;
 RST_VIEW_DRIVE_THROUGH_Y: spritelayout_0_roadstop_y;
 spritelayout_0_roadstop_y;
}
switch (FEAT_ROADSTOPS, SELF, switch_0_roadstop_city_view, view) {
 RST_VIEW_BAY_NE: spritelayout_0_roadstop_ne;
 RST_VIEW_BAY_SE: spritelayout_0_roadstop_se;
 RST_VIEW_BAY_SW: spritelayout_0_roadstop_sw;
 RST_VIEW_BAY_NW: spritelayout_0_roadstop_nw;
 RST_VIEW_DRIVE_THROUGH_X: spritelayout_0_roadstop_x_city;
 RST_VIEW_DRIVE_THROUGH_Y: spritelayout_0_roadstop_y_city;
 spritelayout_0_roadstop_y;
}
switch (FEAT_ROADSTOPS, SELF, switch_0_roadstop_city, town_zone) {//Ensure pavement in the city centre
 TOWNZONE_CENTRE: switch_0_roadstop_city_view;
 TOWNZONE_INNER_SUBURB: switch_0_roadstop_city_view;
 TOWNZONE_OUTER_SUBURB: switch_0_roadstop_city_view;
 TOWNZONE_OUTSKIRT: switch_0_roadstop_city_view;
 TOWNZONE_EDGE: switch_0_roadstop_view;
 switch_0_roadstop_view;
}
//----------------------------------------------------------
//The item block; here the properties for this roadstop are given
//----------------------------------------------------------
item (FEAT_ROADSTOPS, WARE_0, 0x00) {
 property {
  class: "WARE";
  classname: string(STR_CAT_WARE);
  availability_type: RST_AVAILABILITY_TYPE_ALL;
  general_flags: bitmask(RST_GENERAL_FLAG_NO_CATENARY);
  name: string(STR_WARE_0_NAME);
 }
 graphics {//If you always want pavement, use switch_0_roadstop_city_view instead
  default: switch_0_roadstop_city;
 }
}
item (FEAT_ROADSTOPS, WARE_WP_0, 0x01) {
 property {
  class: "WAYP";
  classname: string(STR_CAT_WARE);
  availability_type: RST_AVAILABILITY_TYPE_ALL;
  general_flags: bitmask(RST_GENERAL_FLAG_NO_CATENARY);
  name: string(STR_WARE_0_NAME);
 }
 graphics {//If you always want pavement, use switch_0_roadstop_city_view instead
  default: switch_0_roadstop_city;
 }
}
