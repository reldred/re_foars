}
switch (FEAT_ROADSTOPS, SELF, switch_1_roadstop_city_view, view) {
 RST_VIEW_BAY_NE: spritelayout_1_roadstop_ne;
 RST_VIEW_BAY_SE: spritelayout_1_roadstop_se;
 RST_VIEW_BAY_SW: spritelayout_1_roadstop_sw;
 RST_VIEW_BAY_NW: spritelayout_1_roadstop_nw;
 RST_VIEW_DRIVE_THROUGH_X: spritelayout_1_roadstop_x_city;
 RST_VIEW_DRIVE_THROUGH_Y: spritelayout_1_roadstop_y_city;
 spritelayout_1_roadstop_y;
}
switch (FEAT_ROADSTOPS, SELF, switch_1_roadstop_city, town_zone) {//Ensure pavement in the city centre
 TOWNZONE_CENTRE: switch_1_roadstop_city_view;
 TOWNZONE_INNER_SUBURB: switch_1_roadstop_city_view;
 TOWNZONE_OUTER_SUBURB: switch_1_roadstop_city_view;
 TOWNZONE_OUTSKIRT: switch_1_roadstop_city_view;
 TOWNZONE_EDGE: switch_1_roadstop_view;
 switch_1_roadstop_view;
}
